{
	"name": "svelte-roughviz",
	"version": "0.0.1",
	"description": "Svelte Wrapper for roughViz.js",
	"license": "MIT",
	"author": {
    "name": "GrayFrost",
    "email": "garyfrost4321@gmail.com",
    "url": "https://github.com/GrayFrost"
  },
  "bugs": "https://github.com/GrayFrost/svelte-roughviz/issues",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"format": "prettier --write .",
		"lint": "prettier --check ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"type": "module",
	"repository": {
    "type": "git",
    "url": "git+https://github.com/GrayFrost/svelte-roughviz.git"
  },
	"exports": {
		".": {
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"d3-array": "^3.2.4",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.0.0"
	},
	"keywords": [
		"svelte",
		"roughviz",
		"rough",
		"viz",
		"diagrams",
		"visualization"
	],
	"dependencies": {
		"rough-viz": "^2.0.5"
	}
}
